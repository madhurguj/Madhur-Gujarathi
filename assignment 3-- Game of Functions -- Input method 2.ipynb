   "source": [
    "# Write a Python function to sum all the numbers in a list.\n",
    "\n",
    "Sample List : (8, 2, 3, 0, 7)\n",
    "\n",
    "Expected Output : 20\n",
    "\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 6,
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "20\n"
     ]
    }
   ],
   "source": [
    "def summation(arg):\n",
    "    s=0\n",
    "    for i in arg:\n",
    "        s+=i\n",
    "    return s\n",
    "\n",
    "l=[8,2,3,0,7]\n",
    "print(summation(l))\n",
    "        \n",
    "    "
   ]
  }
